{
  "aws_cognito_user_fields": {
    "aws_account_id": {
      "attribute_data_type": "Number",
      "number_attribute_constraints": {
        "max_value": 12,
        "min_value": 12
      }
    },
    "email": {
      "attribute_data_type": "String",
      "required": true,
      "string_attribute_constraints": {
        "max_length": 256,
        "min_length": 6
      }
    },
    "is_active": {
      "attribute_data_type": "Boolean"
    },
    "linkedin": {
      "attribute_data_type": "String"
    },
    "start_date": {
      "attribute_data_type": "String",
      "string_attribute_constraints": {
        "max_length": 10,
        "min_length": 10
      }
    },
    "twitter": {
      "attribute_data_type": "String"
    }
  }
}
